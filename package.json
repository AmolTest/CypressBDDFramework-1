      "cypress/e2e/[filepath].{js,ts}"
    ]
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^17.1.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress-real-events": "^1.8.1"
